<div class="LC2_date">
  <% locals.years.forEach(function(years, i){%>
  <div class="LC2_date_core <%=locals.type%>">
    <ul class="LC2_date_head">
      <li>
        <span class="LC2_icon LC2_icon_arrow-upper prev-years"></span>
      </li>
      <li>
        <span class="year-value"><%=locals.years[i][0]%>年</span>
        <span>-</span>
        <span class="year-value"><%=locals.years[i][14]%>年</span>
      </li>
      <li>
        <span class="LC2_icon LC2_icon_arrow-next next-years"></span></span>
      </li>
    </ul>
    <div class="LC2_date_content">
      <ul class="LC2_date_list">
        <% years.forEach(function(year, index){%>
        <li class="<%= parseInt(locals.value[i].split('/')[0]) == year ? 'LC2_active' : ''%>">
          <span class="LC2_year" year="<%=year%>"><%=year%></span>
        </li>
        <%})%>
      </ul>
    </div>
    <ul class="LC2_date_footer">
      <li>
        <%if (locals.years.length == 1 || i > 0) {%>
        <a href="#" class="LC2_date_button">确定</a>
        <%}%>
      </li>
    </ul>
  </div>
  <%})%>
</div>